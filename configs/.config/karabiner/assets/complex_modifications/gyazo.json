{
  "title": "Gyazo起動",
  "rules": [
    {
      "description": "Fnキー単独でGyazo起動",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "fn"
          },
          "to_if_alone": [
            {
              "shell_command": "open -a 'Gyazo.app'"
            }
          ]
        }
      ]
    },
    {
      "description": "Print Screenキー単独でGyazo起動",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "print_screen"
          },
          "to_if_alone": [
            {
              "shell_command": "open -a 'Gyazo.app'"
            }
          ]
        }
      ]
    }
  ]
}
